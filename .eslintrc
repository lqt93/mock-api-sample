{
  "extends": ["airbnb"],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "."
        ]
      }
    }
  },
  "rules": {
    "indent": ["error", 2],
    "no-tabs": 0,
    "semi": ["error", "always"],
    "react/button-has-type": 0,
    "react/react-in-jsx-scope": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "import/prefer-default-export": 0
  }
}
